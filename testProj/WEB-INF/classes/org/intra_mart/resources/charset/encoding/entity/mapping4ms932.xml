	</mapping>
	<mapping>
		<source radix="16">2016</source>	// Double Vertical Line
		<revise radix="16">2225</revise>	// Parallel To
	</mapping>
	<mapping>
		<source radix="16">00A2</source>	// Cent Sign
		<revise radix="16">FFE0</revise>	// Fullwidth Cent Sign
	</mapping>
	<mapping>
		<source radix="16">00A3</source>	// Pound Sign
		<revise radix="16">FFE1</revise>	// Fullwidth Pond Sign
	</mapping>
	<mapping>
		<source radix="16">00AC</source>	// Not Sign
		<revise radix="16">FFE2</revise>	// Fullwidth Not Sign
	</mapping>
	<mapping>
		<source radix="16">00A6</source>	// Broken Bar
